    <!--
            Jockey contains a theme for Apex and Nova launchers
            This theme is intended to be used to manually select an icon for Jockey that
            matches the chosen theme

            These strings provide information to Apex & Nova
    -->

    <!-- Basic theme information -->
    <string name="theme_title" tools:ignore="UnusedResources">Jockey Alternate Icons</string>
    <string name="developer_name" tools:ignore="UnusedResources">marverenic</string>
    <string name="theme_description" tools:ignore="UnusedResources">This theme contains alternate icons for Jockey.\n\nDon\'t set this as your launcher\'s theme. You should manually change Jockey\'s icon to your preference if desired.</string>

    <!-- Theme settings -->
    <bool name="config_iconpack" tools:ignore="UnusedResources">true</bool>
    <bool name="config_apexskin" tools:ignore="UnusedResources">false</bool>

    <!-- The feature graphic will appear above the theme description -->
    <string name="theme_feature" tools:ignore="UnusedResources">ic_launcher_black</string>

    <!-- Each theme may have up to 5 preview images -->
    <string name="theme_preview1" tools:ignore="UnusedResources">ic_launcher_red</string>
    <string name="theme_preview2" tools:ignore="UnusedResources">ic_launcher_amber</string>

